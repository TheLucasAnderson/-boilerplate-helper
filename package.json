{
  "name": "boilerplate-helper",
  "version": "0.5.0",
  "author": "github/TheLucasAnderson",
  "description": "A boilerplate for React projects with Typescript and Vite",
  "license": "MIT",
  "main": "./src/main.tsx",
  "repository": {
    "type": "git",
