  },
  "dependencies": {
    "commitizen": "4.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "typescript": "5.4.5",
    "vite": "5.2.13",
    "vite-plugin-eslint": "1.8.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
